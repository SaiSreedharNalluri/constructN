{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/forge/index.jsx",
    "components/container/GenericViewer.jsx",
    "pages/projects/[projectId]/sections/SectionsStyles",
    "utils/GenericViewerTransportLayer.js", "components/container/mapboxViewer.jsx", "pages/error-boundary.jsx", "pages/projects/[projectId]/progress-2d/asset-timeline"  ],
  "exclude": ["node_modules"]
}
